; Engine configuration file.
; It's best edited using the Godot editor.

config_version=5

[application]
config/name="Trilogy"
run/main_scene="res://src/levels/PrototypeLevel.tscn"
config/features=PackedStringArray("4.3")
config/custom_features=PackedStringArray()

[autoload]
GameDirector="*res://autoload/GameDirector.gd"

[rendering]
renderer/rendering_method="forward_plus"
renderer/renderer_name="ForwardPlus"

[input]
move_left={
"deadzone": 0.5,
"events": [ { "type": "KEY", "keycode": 65 }, { "type": "KEY", "keycode": 4194319 }, { "type": "JOY_AXIS", "axis": 0, "axis_value": -1.0 } ]
}
move_right={
"deadzone": 0.5,
"events": [ { "type": "KEY", "keycode": 68 }, { "type": "KEY", "keycode": 4194321 }, { "type": "JOY_AXIS", "axis": 0, "axis_value": 1.0 } ]
}
jump={
"deadzone": 0.5,
"events": [ { "type": "KEY", "keycode": 32 }, { "type": "KEY", "keycode": 4194320 }, { "type": "JOY_BUTTON", "button_index": 0 } ]
}
dash={
"deadzone": 0.5,
"events": [ { "type": "KEY", "keycode": 70 }, { "type": "JOY_BUTTON", "button_index": 1 } ]
}
pause={
"deadzone": 0.5,
"events": [ { "type": "KEY", "keycode": 80 }, { "type": "KEY", "keycode": 4194324 } ]
}
